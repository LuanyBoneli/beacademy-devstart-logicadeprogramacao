Algoritmo "exercicio 12"
// Disciplina   : [Linguagem e Lógica de Programação]
// Autor(a)    : Luany Boneli
// Data atual  : 04/06/2022

Var
   n: INTEIRO;
   numero: INTEIRO;

Inicio
   escreva("Digite o numero que deseja realizar a tabuada: ")
   leia(numero)

   para n de 1 ate 10 faca
      escreval(numero, " x ", n," = ", numero*n)
   fimpara

Fimalgoritmo